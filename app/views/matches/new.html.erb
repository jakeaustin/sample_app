<% provide(:title, 'Create') %>
<h1>Create a Match Up!</h1>

<div class = "row">
	<div class = "span6 offset3">
		
		<%= form_for (@match) do |f| %>
			<%= f.label :matchTitle %>
			<%= f.text_field :matchTitle %>
			<%= f.label :Lpic %>
			<%= f.text_field :Lpic %>
			<%= f.label :Rpic %>
			<%= f.text_field :Rpic %>
			<%= f.label :Ltitle %>
			<%= f.text_field :Ltitle %>
			<%= f.label :Rtitle %>
			<%= f.text_field :Rtitle %>


			<%= f.submit "Create!", 
				      class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
